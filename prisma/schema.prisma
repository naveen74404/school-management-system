generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model School {
  id       Int      @id @default(autoincrement())
  name     String
  address  String
  city     String
  state    String
  contact  String
  image    String
  email_id String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
