generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model School {
  id        Int      @id @default(autoincrement())
  name      String
  address   String
  city      String
  state     String
  contact   String
  image     String
  email_id  String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("schools")
}
